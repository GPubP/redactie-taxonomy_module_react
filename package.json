{
  "name": "@redactie/taxonomy-module",
  "version": "2.4.0",
  "description": "redactie taxonomy module",
  "main": "dist/redactie-taxonomy-module.umd.js",
  "module": "dist/redactie-taxonomy-module.umd.js",
  "jsnext:main": "dist/redactie-taxonomy-module.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "public"
  ],
  "scripts": {
    "prepublishOnly": "npm run test && npm run build",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run trans:extract && webpack --env.prod && npm run build:types",
    "build:w": "npm run trans:extract && webpack -w --env.prod && npm run build:types",
    "build:analyse": "webpack --env.analyse && npm run build:types",
    "lint:scss": "stylelint public/**/*.scss --allow-empty-input",
    "lint:ts": "eslint --cache --cache-location .eslintcache --ext .ts,.tsx public",
    "lint": "npm run lint:scss && npm run lint:ts",
    "test": "jest ./public",
    "trans:extract": "i18next-scanner --config i18next-scanner.config.js",
    "docs": "npm run docs:html && npm run docs:markdown",
    "docs:html": "typedoc --plugin none --entryPoints ./public/index.tsx --entryPoints ./public/lib/api/index.ts --excludePrivate --out docs",
    "docs:markdown": "typedoc --plugin typedoc-plugin-markdown --plugin typedoc-github-wiki-theme --entryPoints ./public/index.tsx --entryPoints ./public/lib/api/index.ts --excludePrivate --theme github-wiki --out docs/markdown"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.antwerpen.be/scm/reda/redactie-taxonomy_module_react.git"
  },
  "publishConfig": {
    "@redactie:registry": "https://nexusrepo.antwerpen.be/repository/npm-private"
  },
  "keywords": [
    "Redactie",
    "React",
    "module",
    "Taxonomy"
  ],
  "author": "glenn verschooren <glenn.verschooren@digipolis.be>",
  "license": "ISC",
  "devDependencies": {
    "@acpaas-ui/react-components": "5.0.9",
    "@acpaas-ui/react-editorial-components": "1.4.1",
    "@babel/core": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/preset-react": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@datorama/akita": "4.22.0",
    "@redactie/form-renderer-module": "1.8.13",
    "@redactie/language-module": "0.0.11",
    "@redactie/module-webpack-plugin": "2.0.0",
    "@redactie/redactie-core": "2.0.0",
    "@redactie/roles-rights-module": "0.8.1",
    "@redactie/translations-module": "1.3.3",
    "@redactie/utils": "2.6.3",
    "@testing-library/jest-dom": "5.0.2",
    "@testing-library/react": "9.4.0",
    "@types/classnames": "2.2.11",
    "@types/jest": "24.9.1",
    "@types/lodash.kebabcase": "4.1.6",
    "@types/ramda": "0.27.32",
    "@types/react": "16.9.19",
    "@types/react-router-dom": "5.1.3",
    "@types/yup": "0.28.3",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "babel-jest": "25.1.0",
    "classnames": "2.2.6",
    "eslint": "7.32.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "formik": "2.1.4",
    "husky": "3.1.0",
    "i18next-scanner": "3.1.0",
    "i18next-scanner-typescript": "1.0.6",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.1.0",
    "ky": "0.25.0",
    "list-to-tree-lite": "0.1.0",
    "node-sass": "4.13.1",
    "prettier": "1.19.1",
    "ramda": "0.27.1",
    "react": "16.12.0",
    "react-router-dom": "5.1.2",
    "rxjs": "6.6.3",
    "stylelint": "13.0.0",
    "stylelint-config-standard": "19.0.0",
    "stylelint-scss": "3.18.0",
    "ts-jest": "25.2.1",
    "typedoc": "^0.22.4",
    "typedoc-github-wiki-theme": "^1.0.0",
    "typedoc-plugin-markdown": "^3.11.14",
    "typescript": "4.6.3",
    "typescript-plugin-css-modules": "2.1.2",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "yup": "0.32.11"
  },
  "dependencies": {}
}
